   "source": [
    "def convert(s):\n",
    "    f = float(s)\n",
    "# formula Applied below\n",
    "    c = (f - 32) * 5/9\n",
    "    return c\n",
    "celsius=input(\"Enter values in Fahrenheit: \")\n",
    "convert(celsius)"
   ]
